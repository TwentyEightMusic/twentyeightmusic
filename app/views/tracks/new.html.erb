<div class="gigs panel callout radius">
  <%= render "error_messages", target: @track %>
  <%= form_for [:bootleg, @track] do |form| %>
    <div>
      <%= form.filepicker_field :filepicker_url %>
      <%= form.text_field :name, placeholder:"Track name" %>
    </div>
    <%= form.submit "Add bootleg" %>
  <% end %>
</div>
